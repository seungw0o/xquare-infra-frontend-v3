{
  "name": "xquare-design-system",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/xquare-design-system/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/xquare-design-system"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.coverageDirectory}"
      ],
      "options": {
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p libs/xquare-design-system/tsconfig.lib.json"
      }
    }
  },
  "tags": []
}